{
  "expo": {
    "name": "React Weather App",
    "slug": "react-weather-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react-icon-png-7.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.VanDeny.ReactNativeWeatherApp",
      "versionCode": 1
    },
    "description": "Testing React Native PogU",
    "githubUrl": "https://github.com/VanDeny/ReactWeatherApp"
  }
}
